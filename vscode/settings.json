{
  "editor.fontSize": 15,
  "editor.rulers": [
    80,
    120
  ],
  "editor.tabSize": 4,
  "editor.trimAutoWhitespace": true,
  "editor.tabCompletion": "on",
  "explorer.openEditors.visible": 0,
  "window.restoreWindows": "none",
  "terminal.external.osxExec": "Hyper.app",
  "terminal.integrated.fontSize": 14,
  "files.autoSave": "onFocusChange",
  "extensions.autoUpdate": true,
  "editor.cursorBlinking": "phase",
  "window.title": "${activeEditorMedium}${separator}${rootName}",
  "files.trimTrailingWhitespace": true,
  "editor.minimap.enabled": false,
  "editor.renderWhitespace": "all",
  "workbench.colorTheme": "Dracula",
  "window.zoomLevel": 0,
  "extensions.ignoreRecommendations": false,
  "go.coverageDecorator": {
    "type": "gutter"
  },
  "search.exclude": {
    "**/node_modules": true,
    "**/bower_components": true,
    "/vendor": true
  },
  "workbench.startupEditor": "newUntitledFile",
  "workbench.fontAliasing": "auto",
  "docker.showExplorer": false,
  "workbench.editor.showIcons": true,
  "explorer.confirmDragAndDrop": false,
  "search.smartCase": true,
  "editor.fontFamily": "FuraCode Nerd Font Mono Retina, Menlo, Monaco, 'Courier New', monospace",
  "editor.fontLigatures": true,
  "files.associations": {
    "*.tmpl": "gotemplate"
  },
  "go.testTimeout": "3m",
  "breadcrumbs.enabled": true,
  "telemetry.enableTelemetry": false,
  "terraform.telemetry.enabled": false,
  "telemetry.enableCrashReporter": false,
  "go.useCodeSnippetsOnFunctionSuggest": true,
  "go.formatTool": "goimports",
  "search.useGlobalIgnoreFiles": true,
  "workbench.editor.highlightModifiedTabs": true,
  "search.showLineNumbers": true,
  "ruby.intellisense": "rubyLocate",
  "ruby.format": "rubocop",
  "ruby.codeCompletion": "rcodetools",
  "ruby.useLanguageServer": true,
  "go.autocompleteUnimportedPackages": true,
  "editor.fontWeight": "600",
  "terminal.integrated.cursorBlinking": true,
  "go.coverOnSingleTest": true,
  "terminal.integrated.cursorStyle": "line",
  "javascript.updateImportsOnFileMove.enabled": "always",
  "go.installDependenciesWhenBuilding": false,
  "files.exclude": {
    "**/.classpath": true,
    "**/.project": true,
    "**/.settings": true,
    "**/.factorypath": true
  },
  "editor.suggestSelection": "first",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "python.jediEnabled": false,
  "team.showWelcomeMessage": false,
  "gitlens.views.repositories.location": "scm",
  "gitlens.views.fileHistory.location": "explorer",
  "gitlens.views.lineHistory.location": "explorer",
  "gitlens.views.compare.location": "gitlens",
  "gitlens.views.search.location": "gitlens",
  "gitlens.keymap": "alternate",
  "vsicons.dontShowNewVersionMessage": true,
  "remote.SSH.configFile": "/Users/sean/.ssh/config"
}
